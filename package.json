{
  "name": "nuxtjs-vk-pixel",
  "type": "module",
  "version": "0.0.4",
  "description": "NuxtJS VK Pixel module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chemist-repo/nuxtjs-vk-pixel.git"
  },
  "author": {
    "name": "Dmitrii Lartsev <https://github.com/chemist-repo/>",
    "url": "https://github.com/chemist-repo"
  },
  "homepage": "https://github.com/chemist-repo/nuxtjs-vk-pixel",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "npm run dev:prepare && nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit",
    "hook:install": "lefthook install"
  },
  "dependencies": {
    "@nuxt/kit": "^4.2.2"
  },
  "peerDependencies": {
    "@nuxt/scripts": "^0.13.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.3.1",
    "@commitlint/config-conventional": "^20.3.1",
    "@commitlint/types": "^20.3.1",
    "@nuxt/devtools": "^3.1.1",
    "@nuxt/eslint-config": "^1.12.1",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.2.2",
    "@nuxt/test-utils": "^3.22.0",
    "@types/node": "latest",
    "changelogen": "^0.6.2",
    "eslint": "^9.39.2",
    "lefthook": "^2.0.13",
    "nuxt": "^4.2.2",
    "typescript": "~5.9.3",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.2.2"
  }
}
